//길이에 따른 연산
function solution(num_list) {
  var answer = 1;
  if (num_list.length <= 10) {
    for (let i = 0; i < num_list.length; i++) {
      answer *= num_list[i];
    }
  } else {
    for (let i = 0; i < num_list.length; i++) {
      answer += num_list[i];
    }
    return answer - 1;
  }
  return answer;
}
