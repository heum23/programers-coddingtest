// 커피 심부름
function solution(order) {
  var answer = 0;
  order.map((num) => {
    if (
      num === "iceamericano" ||
      num === "americanoice" ||
      num === "hotamericano" ||
      num === "americanohot" ||
      num === "americano" ||
      num === "anything"
    ) {
      answer += 4500;
    } else {
      answer += 5000;
    }
  });
  return answer;
}
